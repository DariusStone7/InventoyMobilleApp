{
  "expo": {
    "name": "InventoryApp",
    "slug": "InventoryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logoSansFond.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoSansFond.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dariusstone7.InventoryApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logoSansFond.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "726d7bd4-0638-4945-b0f2-9ea48598b369"
      }
    }
  }
}
